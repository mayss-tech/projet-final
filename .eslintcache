ucer;","C:\\Users\\ASUS\\Desktop\\projet_final\\client\\src\\Components\\JS\\store\\index.js",[],"C:\\Users\\ASUS\\Desktop\\projet_final\\client\\src\\Components\\JS\\actions\\userAction.js",[],"C:\\Users\\ASUS\\Desktop\\projet_final\\client\\src\\Components\\JS\\actions\\restaurantAction.js",[],"C:\\Users\\ASUS\\Desktop\\projet_final\\client\\src\\Components\\JS\\actions\\menuAction.js",[],"C:\\Users\\ASUS\\Desktop\\projet_final\\client\\src\\Components\\JS\\actions\\shopping-cartAction.js",[],"C:\\Users\\ASUS\\Desktop\\projet_final\\client\\src\\Components\\shoppingCart\\cart.jsx",["214","215"],"import React, { useEffect,useState } from \"react\";\r\nimport { useSelector, useDispatch } from \"react-redux\";\r\nimport Bar from \"../Bar/Bar\";\r\nimport { nextStep } from \"../JS/actions/ProgressBar\";\r\nimport {\r\n  removeItem,\r\n  increaseItem,\r\n  decreaseItem,\r\n  totalCart,\r\n} from \"../JS/actions/shopping-cartAction\";\r\nimport { getProfile } from \"../JS/actions/userAction\";\r\nconst Cart = (props) => {\r\n  const cartItems = useSelector((state) => state.cartReducer.cartItems);\r\n  const dispatch = useDispatch();\r\n  const [total, setTotal] = useState();\r\n\r\n  const subTot = () => {\r\n    if (Number(cartItems.reduce((a, b) => a + b.price * b.qtn, 0)) === 0) {\r\n      setTotal(0);\r\n    } else\r\n      setTotal(Number( cartItems.reduce((a, b) => a + b.price * b.qtn, 0))+5);\r\n  };\r\n  useEffect(() => {\r\n    subTot()\r\n  }, [cartItems]);\r\n  useEffect(() => {\r\n    dispatch(getProfile())\r\n  }, []);\r\n\r\n  return (\r\n    <div className=\"cart\">\r\n      <div className=\"shopping-cart cart_header\">\r\n        {cartItems.length === 0 ? (\r\n          <div className=\"cart_p\">\r\n            <p>Votre pannier est vide</p>\r\n          </div>\r\n        ) : (\r\n          <div className=\"cart_p\">\r\n            <p>\r\n              Votre panier contient {cartItems.length} achats{\" \"}\r\n              <i className=\"fas fa-utensils\"